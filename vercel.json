{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@now/node",
        "config": {
          "buildCommand": "npx prisma migrate deploy && npx prisma db seed"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.js"
      }
    ]
  }